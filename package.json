{
  "name": "without-propagation",
  "version": "0.1.0",
  "description": "Stops event propagation",
  "keywords": [
    "event",
    "propagation"
  ],
  "repository": "github:sadorlovsky/without-propagation",
  "license": "MIT",
  "author": {
    "name": "Zach Orlovsky",
    "email": "sadorlovsky@gmail.com",
    "url": "https://orlovsky.dev"
  },
  "files": [
    "dist"
  ],
  "main": "dist/module/index.js",
  "unpkg": "dist/browser/index.js",
  "module": "dist/module/index.js",
  "source": "source/index.ts",
  "browser": "dist/browser/index.js",
  "scripts": {
    "build": "parcel build source/index.ts",
    "test": "tsc --noEmit && xo && ava"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "xo --fix",
    "package.json": "sort-package-json"
  },
  "xo": {
    "extends": "xo-typescript",
    "extensions": [
      "ts"
    ],
    "rules": {
      "@typescript-eslint/indent": [
        "error",
        2
      ],
      "@typescript-eslint/semi": [
        "error",
        "never"
      ]
    }
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@types/sinon": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "ava": "^2.4.0",
    "eslint-config-xo-typescript": "^0.23.0",
    "husky": "^3.1.0",
    "lint-staged": "^10.0.0-beta.7",
    "parcel": "^2.0.0-alpha.3.2",
    "sort-package-json": "^1.32.2",
    "standard": "^14.3.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "window": "^4.2.6",
    "xo": "^0.25.3"
  }
}
